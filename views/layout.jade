doctype html
html(lang='en', ng-app='store')
  head
    meta(charset="utf-8")
    title Untitled
    link(rel="stylesheet", type="text/css", href="navbar/css/normalize.css")
    link(href='http://fonts.googleapis.com/css?family=Rye', rel='stylesheet', type='text/css')
    link(rel="stylesheet", href="navbar/css/component.css")
    link(rel="stylesheet", href="navbar/css/demo.css")
    link(rel="stylesheet", href="navbar/css/icons.css")
    link(rel='stylesheet', type='text/css', href='css/main.css')
    script(src="navbar/js/modernizr.custom.js")

  body
    
    //- navbar originally created by Mary Lou (http://tympanus.net/codrops/author/crnacura/), she's awesome
    div(id='st-container',class='st-container st-effect-11')
      nav(class='st-menu st-effect-11', id='menu-11') 
        h2(class="icon icon-key") Menu!
        ul
          li
            a(class="icon icon-location", ng-href="/#/") Home
          li 
            a(class="icon icon-fire", ng-href="/#/about") About  
          li
            a(class="icon icon-note", ng-href='/#/queue') Queue
          li
            a(class="icon icon-stack", ng-href="/#/portfolio") Portfolio
          li
            a(class="icon icon-cloud", ng-href="/#/shop") Shop

      div(class='st-pusher')

          div(class="menu-top clearfix")
            div(class="st-trigger-effects")
              button(data-effect="st-effect-11" class='menu-tog-btn') 
                span(class='menu-icon') &#9816; Menu 
        

          div(ng-view)


    script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.3/angular.min.js')
    script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.3/angular-route.js')
    script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.3/angular-resource.js')
    script(type='text/javascript', src="navbar/js/classie.js")
    script(type='text/javascript', src="navbar/js/sidebarEffects.js")
    script(type='text/javascript', src='https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js')
    script(type='text/javascript', src='js/main.js')